{
    "name": "collab-corp/laravel-model-routes",
    "description": "Easily generate the urls of your eloquent models.",
    "keywords": [
        "collab-corp",
        "laravel-model-routes"
    ],
    "homepage": "https://github.com/collab-corp/laravel-model-routes",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Kervin Hansen",
            "email": "jonas.kerwin.hansen@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/database": "^5.6",
        "illuminate/routing": "^5.6",
        "illuminate/support": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "CollabCorp\\LaravelModelRoutes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CollabCorp\\LaravelModelRoutes\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
